<body>
    <input type="Button" value="Back" onclick="goback">
    <input type="Button" value="forword" onclick="goforword">
    <script type="text/javascript">
        function goback()
        {
            window.history.back(-1);
        }
        function goforword();
        {
            window.history.forword(+1);
        }
        document.write("")
    </script>
</body>
